{
  "name": "elastalert2-server",
  "version": "5.5.0",
  "description": "A server that runs ElastAlert2 and exposes REST API's for manipulating rules and alerts.",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karql/elastalert2-server.git"
  },
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "dependencies": {
    "@tsoa/runtime": "^6.0.0",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "elasticsearch": "^16.7.3",
    "express": "^4.18.3",
    "joi": "^17.12.1",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "randomstring": "^1.3.0",
    "swagger-ui-express": "^5.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@tsoa/cli": "^6.0.1",
    "@types/bunyan": "^1.8.11",
    "@types/cors": "^2.8.17",
    "@types/elasticsearch": "^5.0.43",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/lodash": "^4.14.202",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^20.11.24",
    "@types/randomstring": "^1.1.12",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon --config nodemon-spec.json\"",
    "build": "tsoa spec-and-routes && tsc",
    "start": "sh ./scripts/start.sh",
    "serve": "nodemon --inspect --watch dist/ -d 1 ./dist/index.js",
    "tsc:watch": "tsc -w",
    "tsc:build": "tsc"
  }
}
